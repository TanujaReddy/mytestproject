<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="230"
    xtype="dialog">
    <items
        jcr:primaryType="nt:unstructured"
        title="Article List"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <list
                jcr:primaryType="cq:Widget"
                border="false"
                height=""
                title="Article List"
                width=""
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <limitGridView
                        jcr:primaryType="cq:Widget"
                        defaultValue="12"
                        fieldDescription="numberOfArticleGridViewDescription"
                        fieldLabel="numberOfArticleGridView"
                        name="./limitGridView"
                        validateOnBlur="true"
                        xtype="numberfield"/>
                    <limitListView
                        jcr:primaryType="cq:Widget"
                        defaultValue="30"
                        fieldDescription="numberOfArticleListViewDescription"
                        fieldLabel="numberOfArticleListView"
                        name="./limitListView"
                        validateOnBlur="true"
                        xtype="numberfield"/>
                    <limitSearchView
                        jcr:primaryType="cq:Widget"
                        defaultValue="50"
                        fieldDescription="numberOfArticleSearchViewDescription"
                        fieldLabel="numberOfArticleSearchView"
                        name="./limitSearchView"
                        validateOnBlur="true"
                        xtype="numberfield"/>
                </items>
            </list>
        </items>
    </items>
</jcr:root>
