<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Map details"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <width
            jcr:primaryType="cq:Widget"
            fieldDescription="Width of Video (default 700)"
            fieldLabel="Width"
            name="./width"
            xtype="textfield"/>
        <height
            jcr:primaryType="cq:Widget"
            fieldDescription="Height of Video (Default 360)"
            fieldLabel="Height"
            name="./height"
            xtype="textfield"/>
        <address
            jcr:primaryType="cq:Widget"
            defaultValue="7-9 Irvine Place, Bella Vista NSW"
            fieldDescription="Search Address"
            fieldLabel="Address"
            name="./address"
            xtype="textfield"/>
    </items>
</jcr:root>
