<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Family Sites Config">
                <items jcr:primaryType="cq:WidgetCollection">
                    <map
                        jcr:primaryType="cq:Widget"
                        fieldDescription="urlInputDescription"
                        fieldLabel="familySiteManagementPanel"
                        hideLabel="false"
                        name="./map"
                        title="familySiteTitleAndLink"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            border="true"
                            hideLabel="true"
                            layout="form"
                            padding="10px"
                            width="1000"
                            xtype="familysitespanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <title-value
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    dName="title"
                                    fieldLabel="familySiteTitleInput"
                                    width="230"
                                    xtype="textfield"/>
                                <href-value
                                    jcr:primaryType="cq:Widget"
                                    allowBlank="false"
                                    dName="href"
                                    fieldLabel="familySiteLinkInput"
                                    width="230"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </map>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Login Config">
                <items jcr:primaryType="cq:WidgetCollection">
                    <country
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the country code my/sg/bn/vn/id/ph/th/au/nz"
                        fieldLabel="Country Code"
                        name="./country"
                        xtype="textfield"/>
                    <errormessage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Error Message for unsuccessful login"
                        fieldLabel="Error Message"
                        name="./errormessage"
                        xtype="textfield"/>
                    <showLogin
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Show Login?"
                        name="./showlogin"
                        type="checkbox"
                        xtype="selection"/>
                </items>
            </tab2>
        </items>
    </items>
</jcr:root>
