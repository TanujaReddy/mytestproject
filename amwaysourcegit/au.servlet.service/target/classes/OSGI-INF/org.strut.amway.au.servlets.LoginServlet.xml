<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0">
    <scr:component name="org.strut.amway.au.servlets.LoginServlet">
        <implementation class="org.strut.amway.au.servlets.LoginServlet"/>
        <service servicefactory="false">
            <provide interface="javax.servlet.Servlet"/>
        </service>
        <property name="sling.servlet.resourceTypes">
            sling/servlet/default
        </property>
        <property name="sling.servlet.selectors">
            amway.login
        </property>
        <property name="sling.servlet.extensions">
            html
        </property>
        <property name="sling.servlet.methods">
            POST
        </property>
        <property name="service.vendor" value="Adobe"/>
        <property name="service.pid" value="org.strut.amway.au.servlets.LoginServlet"/>
        <reference name="loginService" interface="org.strut.amway.core.integration.LoginService" cardinality="1..1" policy="static" bind="bindLoginService" unbind="unbindLoginService"/>
    </scr:component>
</components>
