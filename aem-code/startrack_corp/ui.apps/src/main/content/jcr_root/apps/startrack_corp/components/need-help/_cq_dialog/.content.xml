<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Edit Need Help"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container" class="card-image-multifield needhelp">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">

                <items jcr:primaryType="nt:unstructured">
                
                                   
                     <helptags
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/common/tagspicker"
                        cq-msm-lockable="cq:helptags"
                        fieldLabel="Need Help Tags"
                        name="./cq:helptags"
                        required="{Boolean}true"/>
                        
                                             
                     <helpSupportText
                          jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/foundation/form/textfield"
                          fieldLabel="Help Support Text"
                          name="./helpSupportText"
                          required="true"/>
                                            
                    <helpSupportURL
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                        fieldLabel="Help Support URL"
                        name="./helpSupportURL"
                        rootPath="/content"/>
                        
                    <customerServiceText
                         jcr:primaryType="nt:unstructured"
                         sling:resourceType="granite/ui/components/foundation/form/textfield"
                         fieldLabel="Customer Service Text"
                         name="./customerServiceText"
                         required="true"/>
                        
                    <customerServiceURL
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                        fieldLabel="Customer Service URL"
                        name="./customerServiceURL"
                        rootPath="/content"/>
                </items>
                
                
                        
            </column>
            
            
        </items>
    </content>
</jcr:root>
